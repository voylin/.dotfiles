<?xml version='1.0'?>
<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
<fontconfig>
 <alias>
  <family>sans-serif</family>
  <prefer>
   <family>Source Han Sans JP</family>
   <family>源ノ角ゴシック JP</family>
  </prefer>
 </alias>
 <alias>
  <family>serif</family>
  <prefer>
   <family>Source Han Sans JP</family>
   <family>源ノ角ゴシック JP</family>
  </prefer>
 </alias>
 <dir>~/.fonts</dir>
</fontconfig>
